<header class="header" itemscope itemtype="http://schema.org/LocalBusiness">
  <h1>{{ store.title }}</h1>
  <nav class="nav" role="navigation">
    <a href="{{ store.url }}"><img itemprop="logo" src="{{ 'lineage.svg' | asset_url }}" alt="{{ store.title }}"></a>
    <ul>
      {% for link in linklists.main-menu.links %}
        <li><a class="nav-link" href="{{ link.url }}">{{ link.title }}</a></li>
      {% endfor %}
    </ul>
  </nav>
</header>
